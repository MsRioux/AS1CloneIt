public class MyClass {
    public static void main(String args[]) {
      int age = 44;
      int height = 64 ;
      String name = "Ms. Rioux", animal = "unicorn";

      System.out.println("Hello, my name is " + name + " and I am " + age + " years old.");
      System.out.println("I am " + height + " inches tall and I wish I was a " + animal);
    }
}
